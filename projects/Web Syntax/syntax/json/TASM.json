{
  "color": [
    {"p": "\".+?\"|'.+?'", "r": "string"},
    {"p": ";.*$", "r": "comment"},
    {"p": "\\b[a-f0-9]+h\\b", "r": "magenta", "f": "gmi"},
    {"p": "\\be?[abcd][xhls]\\b", "r": "blue", "f": "gmi"},
    {"p": "\\b[a-z0-9_]+[^\\n]+(proc|endp)|^[^\\n]*[a-z0-9_]+:|ret", "r": "void", "f": "gmi"},
    {"p": "\\.(stack|data|code|exit|model)|,", "r": "orange"},
    {"p": "\\b(main|end)\\b|\\.startup\\b", "r": "main"},
    {"p": "\\b[0-9]+\\b", "r": "blue"},
    {"p": "\\b[a-z0-9_]+\\s+d[bwdqt]\\b", "r": "yellow", "f": "gmi"},
    {"p": "\\b(mov|int|lea|cmp|jmp|je|jz|call|add)\\b", "r": "func", "f": "gmi"},
    {"p": "\\[[a-z0-9_]+\\]", "r": "case", "f": "gmi"}
  ],
  "reform": [
    {"p": "^\\s+(?=\\b[a-z0-9_]+:)", "r": "", "f": "gmi"},
    {"p": "^[ \\t]*(?=\\b(mov|int|lea|cmp|jmp|je|jz|call)\\b)", "r": "\t", "f": "gmi"},
    {"p": "(?<=\\b(mov|int|lea|cmp|jmp|je|jz|call)\\b)\\s+", "r": "\t\t", "f": "gmi"}
  ]
}
