{
  "color": [
    {"p": "(?<!class=)\"(?!>).+?(?<!class=)\"(?!>)|\".+?\"", "r": "string"},
    {"p": "//.*$", "r": "comment"},
    {"p": "(?<=\\bfunction) +[a-zA-Z0-9_]*?(?=\\()", "r": "magenta"},
    {"p": "\\b[a-zA-Z0-9_]+?(?=\\()", "r": "yellow"},
    {"p": "\\basd\\b", "r": "blue"},
    {"p": "\\bthis\\b|\\bnew\\b|\\bconst\\b|\\blet\\b|\\bfunction\\b", "r": "orange"}
  ],
  "reform": [
    {"p": "^ *(?=let)", "r": "    "}
  ]
}
