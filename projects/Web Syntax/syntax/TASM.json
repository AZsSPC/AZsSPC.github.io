  "reform": [
    {"p": "(?<=endp)\\s*", "r": "\n\n"},
    {"p": "^\\s*(?=\b(mov|int|([a-zA-Z0-9]+\\s+(d[bwdqt]|D[BWDQT])))\b)", "r": "\t"},
    {"p": "(?<=\b(mov|int)\b)\\s*", "r": "\t\t"}
  ]
}